<style>
table {
  border-collapse: collapse;
}

table, th, td {
  border: 1px solid black;
  padding: 5px;
}
</style>
</head>
<body>

<table>
  <thead>
    <tr>
      <th>&times;</th>
      <th>1</th>
      <th>2</th>
      <th>3</th>
      <th>4</th>
      <th>5</th>
      <th>6</th>
      <th>7</th>
      <th>8</th>
      <th>9</th>
      <th>10</th>
      <th>11</th>
      <th>12</th>
    </tr>
  </thead>
  <tbody id="multiplication-table-body">
  </tbody>
</table>

<script>
function createMultiplicationTable(n) {
  var tableBody = document.getElementById('multiplication-table-body');
  for (var i = 1; i <= n; i++) {
    var row = document.createElement('tr');
    for (var j = 0; j <= n; j++) {
      var cell = document.createElement('td');
      if (j === 0) {
        cell.textContent = i;
      } else {
        cell.textContent = i * j;
      }
      row.appendChild(cell);
    }
    tableBody.appendChild(row);
  }
}

createMultiplicationTable(12);
</script>

</body>
</html>